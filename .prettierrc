{
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "endOfLine": "auto",
    "trailingComma": "es5",
    "arrowParens": "always",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "jsxSingleQuote": false,
    "tsxSingleQuote": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "vueIndentScriptAndStyle": true,
    "requirePragma": ,
    "plugins": ["prettier-plugin-tailwindcss"]
}